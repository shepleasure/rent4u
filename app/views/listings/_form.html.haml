%script
	$(document).ready(function() {

	function check_to_hide_or_show_add_link() {
	if ($('#listing_attachments .nested-fields').length == 6) {
	$('#listing_attachments .links a').hide("slow");
	} else {
	$('#listing_attachments .links a').show( "slow" );
	}
	}

	$('#listing_attachments').bind('cocoon:after-insert', function() {
	check_to_hide_or_show_add_link();
	});

	$('#listing_attachments').bind('cocoon:after-remove', function() {
	check_to_hide_or_show_add_link();
	});

	check_to_hide_or_show_add_link();

	$("input[name='listing[security]']").click(function() {
	if ($("#listing_security_yes").is(":checked")) {
	$("#security-amount").show();
	$("#listing_security_amount").attr('required');
	} else {
	$("#security-amount").hide();
	$("#listing_security_amount").removeAttr('required');
	}
	});
	});
